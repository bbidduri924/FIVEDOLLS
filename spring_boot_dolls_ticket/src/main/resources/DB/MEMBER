CREATE TABLE C##FIVEDOLLS.MEMBER
(
    CUST_ID             VARCHAR2(20) NOT NULL,
    CUST_PASSWORD       VARCHAR2(200),
    CUST_NAME           VARCHAR2(15),
    ADMINISTRATOR_YN    VARCHAR2(8),
    CUST_RESIDENTNUMBER VARCHAR2(16),
    CUST_EMAIL          VARCHAR2(30),
    CUST_POINT          NUMBER(20),
    CUST_PHONENUMBER    VARCHAR2(17),
    CUST_ZIPCODE        VARCHAR2(8),
    CUST_ADDRESS        VARCHAR2(80),
    CUST_DETAILADDRESS  VARCHAR2(50),
    CUST_STATUS         VARCHAR2(6),
    CUST_IPADDRESS      VARCHAR2(15),
    FIRST_CHANGE_DATE   DATE,
    LAST_CHANGE_DATE    DATE
)
TABLESPACE USERS
STORAGE
(
    INITIAL 64K
    NEXT 1M
)
NOCOMPRESS;

ALTER TABLE C##FIVEDOLLS.MEMBER
ADD PRIMARY KEY (CUST_ID);
