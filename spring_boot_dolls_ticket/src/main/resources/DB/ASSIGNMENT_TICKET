CREATE TABLE C##FIVEDOLLS.ASSIGNMENT_TICKET
(
    NOTICE_ID                       VARCHAR2(20) NOT NULL,
    PERFORMANCE_ID                  VARCHAR2(20) NOT NULL,
    RESERVATION_SEAT_INFORMATION    VARCHAR2(5) NOT NULL,
    RESERVATION_NUMBER              VARCHAR2(60) NOT NULL,
    RECEIVE_CUST_ID                 VARCHAR2(20),
    SOLD_YN                         CHAR(1) NOT NULL,
    FIRST_CHANGE_DATE               DATE NOT NULL,
    LAST_CHANGE_DATE                DATE NOT NULL
)
TABLESPACE USERS
STORAGE
(
    INITIAL 64K
    NEXT 1M
)
NOCOMPRESS;

ALTER TABLE C##FIVEDOLLS.ASSIGNMENT_TICKET
ADD CONSTRAINT XPKASSIGNMENT_TICKET PRIMARY KEY (NOTICE_ID,PERFORMANCE_ID,RESERVATION_SEAT_INFORMATION);
