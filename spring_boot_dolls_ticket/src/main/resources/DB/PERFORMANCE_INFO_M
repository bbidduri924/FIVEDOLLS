CREATE TABLE "C##FIVEDOLLS"."PERFORMANCE_INFO_M" 

   (	"PERFORMANCE_ID" VARCHAR2(20 BYTE) NOT NULL, 
		"PERFORMANCE_KIND_CD" VARCHAR2(50 BYTE), 
		"PERFORMANCE_RATING_CODE" NUMBER(2,0), 
		"PERFORMANCE_NAME" VARCHAR2(200 BYTE), 
		"PERFORMANCE_PRICE_R" NUMBER(20,0), 
		"PERFORMANCE_ZIPCODE" VARCHAR2(8 BYTE), 
		"PERFORMANCE_ADDRESS" VARCHAR2(80 BYTE), 
		"PERFORMANCE_DETAILADDRESS" VARCHAR2(200 BYTE), 
		"PERFORMANCE_DATE" DATE, 
		"RESERVATION_OPEN_EXPECTED_DATE" DATE, 
		"PERFORMANCE_IMAGE_PATH" VARCHAR2(200 BYTE), 
		"FIRST_CHANGE_DATE" DATE, 
		"LAST_CHANGE_DATE" DATE, 
		"PERFORMANCE_TIME" NUMBER(3,0), 
		"PERFORMANCE_PRICE_S" NUMBER(20,0), 
		"PERFORMANCE_DATE2" DATE
   )
TABLESPACE USERS
STORAGE
(
    INITIAL 64K
    NEXT 1M
)
NOCOMPRESS;

ALTER TABLE C##FIVEDOLLS.PERFORMANCE_INFO_M
ADD PRIMARY KEY (PERFORMANCE_ID);
