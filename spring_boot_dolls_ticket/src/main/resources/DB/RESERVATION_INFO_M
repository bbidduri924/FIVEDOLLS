CREATE TABLE C##FIVEDOLLS.RESERVATION_INFO_M
(
    RESERVATION_NUMBER              VARCHAR2(60) NOT NULL,
    PERFORMANCE_ID                  VARCHAR2(20),
    CUST_ID                         VARCHAR2(20),
    RESERVATION_SEAT_INFORMATION    VARCHAR2(5),
    RESERVATION_STATUS              VARCHAR2(10),
    FIRST_CHANGE_DATE               DATE,
    LAST_CHANGE_DATE                DATE
)
TABLESPACE USERS
STORAGE
(
    INITIAL 64K
    NEXT 1M
)
NOCOMPRESS;

ALTER TABLE C##FIVEDOLLS.RESERVATION_INFO_M
ADD PRIMARY KEY (RESERVATION_NUMBER);

ALTER TABLE C##FIVEDOLLS.RESERVATION_INFO_M
ADD CONSTRAINT RESERVATION_INFO_M_FK1 FOREIGN KEY (PERFORMANCE_ID) REFERENCES C##FIVEDOLLS.PERFORMANCE_INFO_M (PERFORMANCE_ID);

ALTER TABLE C##FIVEDOLLS.RESERVATION_INFO_M
ADD CONSTRAINT RESERVATION_INFO_M_FK2 FOREIGN KEY (CUST_ID) REFERENCES C##FIVEDOLLS.MEMBER (CUST_ID);
